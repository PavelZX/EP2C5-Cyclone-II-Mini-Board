library IEEE;
use IEEE.STD_LOGIC_1164.ALL;

entity Blink1 is
	Port (PB : in STD_LOGIC;
		LED1 : out STD_LOGIC;
		LED2 : out STD_LOGIC;
		LED3 : out STD_LOGIC;
		LED4 : out STD_LOGIC);
end Blink1;

architecture Behavioral of Blink1 is
begin
	LED1 <= PB;
	LED2 <= not PB;
	LED3 <= PB;
	LED4 <= not PB;
	
end behavioral;
